{
    "manifest_version": 2,
    "version": "0.0.1",
    "name": "Extensão do vitor",
    "description": "Automatize a comunicação com o DETRAN do seu estado",
    "author": "Matheus Cruz (mcruzdev), Oiceotse Moura",
    "permissions": ["webRequest"],
    "browser_action": {
        "default_popup": "modules/main/index.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.detran.pe.gov.br/primeira-habilitacao"],
            "js": ["./modules/background/background.js"]
        }
    ]
}